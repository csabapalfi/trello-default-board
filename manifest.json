{
	"name": "Trello Default Board",
	"version": "1.0.0",
	"description": "Redirects the Trello homepage to my default board",
	"homepage_url": "https://github.com/csabapalfi/trello-default-board",
	"manifest_version": 2,
	"minimum_chrome_version": "50",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"https://trello.com/"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	}
}
